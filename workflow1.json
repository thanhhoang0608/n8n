    {
      "parameters": {
        "mode": "runOnceForEachItem",
        "jsCode": "const input = $input.item.json;\nconst binaryData = $binary[\"data\"];\n\n// H√†m x√°c ƒë·ªãnh tr·∫°ng th√°i theo n·ªôi dung\nfunction determineStatusBasedOnContent(itemContent, type = 'Default') {\n    let status = 'Ch∆∞a x√°c ƒë·ªãnh';\n\n    if (typeof itemContent === 'string' && itemContent.length > 0) {\n        if (type === 'Ads') {\n            if (itemContent.includes('pub-') && itemContent.includes(',')) {\n                status = 'Ho·∫°t ƒë·ªông (Ads.txt t√¨m th·∫•y trong n·ªôi dung)';\n            } else if (itemContent.includes('<html') && itemContent.includes('<body') && itemContent.includes('<title>n8n.io - Workflow Automation</title>')) {\n                status = `Kh√¥ng t√¨m th·∫•y (${type} b·ªã tr·∫£ v·ªÅ HTML c·ªßa n8n)`;\n            } else if (itemContent.includes('<html') && itemContent.includes('<body')) {\n                status = `Kh√¥ng ph·∫£i Ads.txt (N·ªôi dung l√† HTML kh√°c)`;\n            } else {\n                status = 'Kh√¥ng ph·∫£i Ads.txt (N·ªôi dung Text kh√¥ng ph√π h·ª£p)';\n            }\n        } else if (itemContent.includes('<html') && itemContent.includes('<body') && itemContent.includes('<title>n8n.io - Workflow Automation</title>')) {\n            status = `Kh√¥ng t√¨m th·∫•y (${type} b·ªã tr·∫£ v·ªÅ HTML c·ªßa n8n)`;\n        } else if (type === 'Sitemap') {\n            if (itemContent.includes('<?xml') && itemContent.includes('<sitemap')) {\n                status = 'Ho·∫°t ƒë·ªông (XML Sitemap)';\n            } else if (itemContent.includes('sitemap') || itemContent.includes('.xml')) {\n                status = 'Kh·∫£ nƒÉng l√† Sitemap (N·ªôi dung kh√¥ng ph·∫£i XML chu·∫©n)';\n            } else if (itemContent.includes('<html') && itemContent.includes('<body')) {\n                status = 'Kh√¥ng ph·∫£i Sitemap (N·ªôi dung l√† HTML)';\n            } else {\n                status = 'Kh√¥ng ph·∫£i Sitemap (N·ªôi dung Text kh√¥ng ph√π h·ª£p)';\n            }\n        } else if (type === 'Robots.txt') {\n            if (itemContent.includes('User-agent:') || itemContent.includes('Disallow:') || itemContent.includes('Allow:')) {\n                status = 'Ho·∫°t ƒë·ªông (Robots.txt)';\n            } else if (itemContent.includes('<html') && itemContent.includes('<body')) {\n                status = 'Kh√¥ng ph·∫£i Robots.txt (N·ªôi dung l√† HTML)';\n            } else {\n                status = 'Kh√¥ng ph·∫£i Robots.txt (N·ªôi dung Text kh√¥ng ph√π h·ª£p)';\n            }\n        } else {\n            status = 'N·ªôi dung kh√¥ng x√°c ƒë·ªãnh';\n        }\n    } else {\n        status = 'Kh√¥ng c√≥ ph·∫£n h·ªìi ho·∫∑c n·ªôi dung r·ªóng';\n    }\n\n    return { status };\n}\n\n// Tr√≠ch xu·∫•t sitemap URL ƒë·∫ßu ti√™n\nfunction extractSitemapURL(content) {\n    if (typeof content === 'string') {\n        const match = content.match(/<loc>(.*?)<\\/loc>/);\n        if (match && match[1]) {\n            return match[1].trim();\n        }\n    }\n    return '';\n}\n\n// X√°c ƒë·ªãnh tr·∫°ng th√°i Favicon d·ª±a v√†o binary metadata\nfunction determineFaviconStatusFromBinary(binary) {\n    if (!binary) return \"Kh√¥ng c√≥ d·ªØ li·ªáu favicon\";\n\n    const mime = (binary.mimeType || \"\").toLowerCase().trim();\n    const fileName = (binary.fileName || \"\").toLowerCase().trim();\n    const fileExt = (binary.fileExtension || \"\").toLowerCase().trim();\n\n    if (mime.includes(\"text/html\") || fileExt === \"html\") {\n        return \"Kh√¥ng t√¨m th·∫•y Favicon (Tr·∫£ v·ªÅ HTML l·ªói)\";\n    }\n\n    if (fileName === \"favicon.ico\" && (mime.includes(\"icon\") || fileExt === \"ico\")) {\n        return \"T√¨m th·∫•y Favicon (H·ª£p l·ªá)\";\n    }\n\n    if (mime.startsWith(\"image/\")) {\n        return \"T√¨m th·∫•y Favicon (L√† ·∫£nh nh∆∞ng kh√¥ng ph·∫£i ICO chu·∫©n)\";\n    }\n\n    return \"Kh√¥ng x√°c ƒë·ªãnh ƒë∆∞·ª£c Favicon (Lo·∫°i t·ªáp kh√¥ng r√µ r√†ng)\";\n}\n\n// T·ªïng h·ª£p k·∫øt qu·∫£\nconst now = new Date();\nconst lastChecked = now.toISOString();\n\nconst sitemapResult = determineStatusBasedOnContent(input.sitemap, 'Sitemap');\nconst robotsResult = determineStatusBasedOnContent(input.robots, 'Robots.txt');\nconst adsResult = determineStatusBasedOnContent(input.ads, 'Ads');\nconst sitemapURL = extractSitemapURL(input.sitemap);\nconst faviconStatus = determineFaviconStatusFromBinary(binaryData);\n\n// Tr·∫£ k·∫øt qu·∫£\nreturn {\n    json: {\n        row_number: input.row_number,\n        Domain: input.Domain,\n        \"Sitemap Status\": sitemapResult.status,\n        \"Sitemap URL\": sitemapURL,\n        \"Robots.txt Status\": robotsResult.status,\n        \"Favicon Status\": faviconStatus,\n        \"Ads Status\": adsResult.status,\n        \"Last Checked\": lastChecked,\n        \"isSitemapRedirectedExternal\": false\n    }\n};\n"
      },
